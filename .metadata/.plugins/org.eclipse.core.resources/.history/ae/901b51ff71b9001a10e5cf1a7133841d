package com.ms.boot.ConvertCurrencyms.model;  
import java.math.BigDecimal;  
public class ConvertCurrencyDTO 
{  
private Long id;  
private String countrycode;  
private BigDecimal ConversionMultiple;  
private BigDecimal amount;  
private BigDecimal totalCalculatedAmount;   
//default constructor  
public ConvertCurrencyDTO()  
{     
}  
//creating constructor  
public ConvertCurrencyDTO(Long id, String countrycode, BigDecimal conversionMultiple, BigDecimal amount, BigDecimal totalCalculatedAmount)   
{  
super();  
this.id = id;  
this.countrycode=countrycode;
ConversionMultiple = conversionMultiple;  
this.amount = amount;  
this.totalCalculatedAmount = totalCalculatedAmount;  
}  
//creating setters and getters  
public Long getId()   
{  
return id;  
}  
public void setId(Long id)   
{  
this.id = id;  
}  

public BigDecimal getConversionMultiple()   
{  
return ConversionMultiple;  
}  
public void setConversionMultiple(BigDecimal conversionMultiple)   
{  
ConversionMultiple = conversionMultiple;  
}  
public BigDecimal getQuantity()   
{  
return quantity;  
}  
public void setQuantity(BigDecimal quantity)   
{  
this.quantity = quantity;  
}  
public BigDecimal getTotalCalculatedAmount()   
{  
return totalCalculatedAmount;  
}  
public void setTotalCalculatedAmount(BigDecimal totalCalculatedAmount)   
{  
this.totalCalculatedAmount = totalCalculatedAmount;  
}  
public int getPort()   
{  
return port;  
}  
public void setPort(int port)   
{  
this.port = port;  
}  
}  